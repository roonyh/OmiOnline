application: omionline
version: 1
runtime: python27
api_version: 1
threadsafe: yes

handlers:
- url: /favicon\.ico
  static_files: favicon.ico
  upload: favicon\.ico

- url: /images
  static_dir: images

- url: /css/(.*\.css)
  mime_type: text/css
  static_dir: static/css/
  upload: static/css/(.*\.css)

- url: /js/(.*\.js)
  mime_type: text/javascript
  static_dir: static/js/
  upload: static/js/(.*\.js)
      
- url: .*
  script: omionline.app

libraries:
- name: webapp2
  version: "2.5.2"
